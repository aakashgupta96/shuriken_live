<%= form_for(@post,:url => "/posts/objects",:method=>"post") do |f| %>
  <% if @post.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@post.errors.count, "error") %> prohibited this post from being saved:</h2>

      <ul>
      <% @post.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :key %><br>
    <%= f.text_field :key, required: true %>
  </div>
  <div class="field">
    <%= f.label :title %><br>
    <%= f.text_field :title, required: true %>
  </div>
  <div class="field">
    <%= f.label :comparisons %><br>
    <%= f.text_field :comparisons, required: true %>
  </div>
  <div class="field">
    <%= f.label :background_image %><br>
    <%= f.file_field :background, required: true %>
  </div>
  <div class="field">
    <%= f.label :Publish_time %><br>
    <%= f.datetime_select :start_time,start_year: Time.now.year, ampm: true, required: true %>
  </div>
  <div class="field">
    <%= f.label :duration %><br>
    <%=  f.time_select :duration, start_hour: 0 , end_hour: 3%>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>